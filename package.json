{
  "name": "seisplotjs-model",
  "version": "1.2.0-alpha.1",
  "description": "Classes for various objects used in seisplotjs packages.",
  "main": "./lib/model.js",
  "dependencies": {
    "moment": "^2.20.1",
    "js-quantities": "1.7.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "qunit -c ./lib/model.js -t ./test/*.js",
    "compile": "babel --presets env -d lib/ src/",
    "standalone": "browserify src/model.js --standalone seisplotjs_model -o seisplotjs_model_${npm_package_version}_standalone.js -t [ babelify --presets [ env ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-model.git"
  },
  "keywords": [
    "seismology",
    "seismogram"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-model/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-model",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  }
}
