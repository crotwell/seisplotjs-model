{
  "name": "seisplotjs-model",
  "version": "1.0.2-alpha.2",
  "description": "Classes for various objects used in seisplotjs packages.",
  "main": "./lib/model.js",
  "dependencies": {},
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "qunit -c ./lib/model.js -t ./test/index.js",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/*.js --standalone seisplotjs_model -o seisplotjs_model_${npm_package_version}_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-model.git"
  },
  "keywords": [
    "seismology",
    "seismogram"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-model/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-model",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  }
}
